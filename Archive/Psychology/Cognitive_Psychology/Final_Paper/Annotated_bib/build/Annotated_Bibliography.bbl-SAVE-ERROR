      \strng{namehash}{15e2e6c2971360cdd251429f86e0c93d}
      \strng{fullhash}{15e2e6c2971360cdd251429f86e0c93d}
      \strng{fullhashraw}{15e2e6c2971360cdd251429f86e0c93d}
      \strng{bibnamehash}{15e2e6c2971360cdd251429f86e0c93d}
      \strng{authorbibnamehash}{15e2e6c2971360cdd251429f86e0c93d}
      \strng{authornamehash}{15e2e6c2971360cdd251429f86e0c93d}
