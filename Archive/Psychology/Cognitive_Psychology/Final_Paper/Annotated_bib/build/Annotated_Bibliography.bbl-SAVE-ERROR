      \strng{namehash}{7e6eb9420b3927006e9c1744b52c0e61}
      \strng{fullhash}{509b0f28ba25da46cf0ce51852f46752}
      \strng{fullhashraw}{509b0f28ba25da46cf0ce51852f46752}
      \strng{bibnamehash}{509b0f28ba25da46cf0ce51852f46752}
