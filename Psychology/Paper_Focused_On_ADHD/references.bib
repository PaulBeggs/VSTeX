@article{harrison_accommodation_2022,
  abstract     = {Students with attention-deficit/hyperactivity disorder ({ADHD}) may be entitled to academic accommodations in postsecondary education. Disability Services Offices ({DSOs}) in Canada state that objective evidence of functional impairment is required before providing academic accommodations. This study set out to determine whether postsecondary disability service providers use objective, third-party data when making accommodation decisions. Providers were asked if they would grant extra time accommodations to a fictitious prospective student. The student self-reported attention and academic problems that emerged during {COVID} restrictions, and stated that extra time helped her earn better grades and reduced her anxiety. While her neuropsychological report suggested a superficial similarity to {ADHD} and contained accommodation recommendations, it lacked objective evidence supporting either an {ADHD} diagnosis or functional impairments that would justify extra time accommodations. Despite the lack of current or historical functional impairment, 100\% of {DSO} decision-makers confirmed that they would grant extra time accommodations to this student. Results suggest that {DSOs}' accommodation decisions are not based on evidence of functional impairment, but rather rely primarily on student self-report and the recommendations of a professional. As such, the current system for determining reasonable accommodations is flawed and inequitable, offering non-impaired individuals access to supports and services that may privilege them over their similarly abled peers. Postsecondary institutions must either develop more defensible methods of disability determination or provide all students with access to accommodations to create a more equitable learning environment.},
  annotation   = {Use this article as a “defense against supporting {ADHD}-related care.” You could possibly delve into further discussion about accommodations, and how that affects the perception of {ADHD}.
                  },
  author       = {Harrison, Allyson G. and Armstrong, Irene},
  date         = {2022-12},
  doi          = {10.1007/s12207-022-09461-1},
  file         = {Full Text:files/49/Harrison and Armstrong - 2022 - Accommodation Decision-Making for Postsecondary Students with ADHD Treating the Able as Disabled.pdf:application/pdf},
  issn         = {1938-971X, 1938-9728},
  journaltitle = {Psychological Injury and Law},
  keywords     = {Against\_ADHD, Anti-Accommodations, Empirical, Supplementary},
  langid       = {english},
  number       = {4},
  pages        = {367--384},
  shortjournal = {Psychol. Inj. and Law},
  shorttitle   = {Accommodation Decision-Making for Postsecondary Students with {ADHD}},
  title        = {Accommodation Decision-Making for Postsecondary Students with {ADHD}: Treating the Able as Disabled},
  url          = {https://link.springer.com/10.1007/s12207-022-09461-1},
  urldate      = {2024-11-16},
  volume       = {15}
}

@article{weyandt_adhd_2006,
  abstract     = {Objective: According to the American Psychiatric Association, 3\% to 7\% of the school-age population has {ADHD} and many children continue to display significant symptoms throughout adolescences and adulthood. Relative to the childhood literature, less is known about {ADHD} in adults, especially college students with {ADHD}. The principle purpose of this review articles is to summarize the major research findings concerning {ADHD} in the college student population with regard to prevalence of symptoms, neuropsychological and psychological functioning. Overall, findings suggest that college students with {ADHD} are at greater risk for academic and psychological difficulties, and they perform similar to non-{ADHD} controls on many neuropsychological tasks. These findings are preliminary, however, and are tempered by the small number of studies that have been conducted as well as the methodological limitations of these studies. 
                  
  Conclusion: Future research using larger sample sizes, rigorous assessment criteria, and a longitudinal design is needed to better understand the psychological, academic, and neuropsychological functioning of college students with {ADHD}. Studies are also needed to elucidate the effects of pharmacological and nonpharmacological effects of treatment on the functioning of college students with this disorder.},
