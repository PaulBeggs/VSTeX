  pages        = {9--19},
  rights       = {https://journals.sagepub.com/page/policies/text-and-data-mining-license},
  shortjournal = {J Atten Disord},
  title        = {{ADHD} in College Students},
  url          = {https://journals.sagepub.com/doi/10.1177/1087054705286061},
