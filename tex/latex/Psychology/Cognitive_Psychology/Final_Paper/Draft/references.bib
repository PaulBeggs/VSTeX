  file         = {Full Text:files/81/Ginapp et al. - 2023 - “Dysregulated not deficit” A qualitative study on symptomatology of ADHD in young adults.pdf:application/pdf},
  issn         = {1932-6203},
  journaltitle = {{PLOS} {ONE}},
  keywords     = {Empirical, Primary, Cognitive},
  langid       = {english},
  number       = {10},
